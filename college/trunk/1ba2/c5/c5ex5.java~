import tcdIO.*; 

class c5ex5 {
  public static void main (String args[]) {
    Terminal terminal;
    Date d;
	 int id,im,iy;

    terminal = new Terminal("Chapter 5: Exercise 5");

    iy = terminal.readInt("Enter the year ");

	 d = new Date(0,0,iy);
  
    if(d.isLeapYear()){
      terminal.println("The year is a leap year");
    }
    else{
      terminal.println("The year is not a leap year");
    }

 }
}   
