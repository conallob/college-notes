class Time {
	private int hours;  
	private int minutes;  

	public Time(int hoursToSet, int minutesToSet) {
	  hours = hoursToSet;
	  minutes = minutesToSet;
	}

	public int getHours() {
	 return hours;
	} 

	public int getMinutes() {
	 return minutes;
	} 

	public void addDuration(Duration durationToAdd){
		hours += durationToAdd.getHours();
		minutes += durationToAdd.getMinutes();
		hours += minutes/60;
		minutes = minutes%60;
		hours = hours%24;
	}
}
